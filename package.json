{
  "name": "cypress-bdd",
  "version": "1.0.0",
  "description": "cypress with cucumber",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:electron": "cypress run --browser electron",
    "cy:run-dashboard": "cypress run --record --key f43659ea-86c3-4aff-b6ea-a603fa77c482",
    "test:cleannih": "rm -rf public/assets",
    "test:real": "npm run test:cleannih && cypress run",
    "test:chrome": "./node_modules/.bin/cypress-tags run -e TAGS='@ayam'",
    "combine:report": "mochawesome-merge ./mochawesome-report/*.json -o index.json",
    "generate:report": "marge index.json --reportDir public --assetsDir public/assets --reportPageTitle index.html",
    "cleanup": "rm -rf mochawesome-report/",
    "merge_reports": "mochawesome-merge mochawesome-report/*.json > output.json",
    "mochawesome_report": "marge --reportDir mochawesome-report -t Cypress_Test_Report -p Cypress_HMTL_Report -f Cypress_HMTL_Report --timestamp mmddyyyy_HHMMss --charts true output.json",
    "cy:report": "npm run cleanup && cypress run && npm run merge_reports && npm run mochawesome_report"
  },
  "keywords": [
    "Cypress",
    "BDD",
    "Cucumber"
  ],
  "author": "johannes",
  "license": "ISC",
  "dependencies": {
    "cypress": "^7.6.0",
    "cypress-cucumber-preprocessor": "^4.1.2",
    "cypress-xpath": "^1.6.2",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "prettier": "^2.3.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
